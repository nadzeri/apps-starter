{
  "name": "@repo/data",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:local:drop": "drizzle-kit drop",
    "db:local:seed": "node scripts/seed.js",
    "db:local:push": "drizzle-kit push",
    "db:local:teardown": "node scripts/teardown.js",
    "db:prod:push": "NODE_ENV=production drizzle-kit push"
  },
  "exports": {
    "./db": {
      "types": "./src/db/index.ts",
      "default": "./dist/db/index.js"
    },
    "./stripe": {
      "types": "./src/services/stripe.ts",
      "default": "./dist/services/stripe.js"
    },
    "./repos": {
      "types": "./src/repos/index.ts",
      "default": "./dist/repos/index.js"
    },
    "./services": {
      "types": "./src/services/index.ts",
      "default": "./dist/services/index.js"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./dist/types/index.js"
    },
    "./invariants": {
      "types": "./src/invariants/index.ts",
      "default": "./dist/invariants/index.js"
    },
    "./aggregates": {
      "types": "./src/domain/aggregates/index.ts",
      "default": "./dist/domain/aggregates/index.js"
    },
    "./constants": {
      "types": "./src/lib/constants/index.ts",
      "default": "./dist/lib/constants/index.js"
    },
    "./errors": {
      "types": "./src/lib/errors/index.ts",
      "default": "./dist/lib/errors/index.js"
    },
    "./utils": {
      "types": "./src/lib/utils/index.ts",
      "default": "./dist/lib/utils/index.js"
    },
    "./use-cases": {
      "types": "./src/usecases/index.ts",
      "default": "./dist/usecases/index.js"
    },
    "./viewQueries": {
      "types": "./src/viewQueries/index.ts",
      "default": "./dist/viewQueries/index.js"
    },
    "./booking": {
      "types": "./src/domain/aggregates/entities/booking.ts",
      "default": "./dist/domain/aggregates/entities/booking.js"
    },
    "./policies": {
      "types": "./src/policies/index.ts",
      "default": "./dist/policies/index.js"
    }
  },
  "dependencies": {
    "@effect-aws/client-s3": "^1.9.5",
    "@effect/data": "^0.18.7",
    "@effect/platform": "^0.80.9",
    "@effect/platform-node": "^0.76.18",
    "@kinde-oss/kinde-auth-nextjs": "2.3.10",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/instrumentation": "^0.201.0",
    "@opentelemetry/instrumentation-pg": "^0.53.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@supabase/supabase-js": "^2.45.6",
    "@trpc/client": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "@types/luxon": "^3.4.2",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.1",
    "drizzle-zod": "^0.5.1",
    "effect": "^3.13.6",
    "export-to-csv": "^1.4.0",
    "flat": "5.0.2",
    "iso-639-1": "^3.1.2",
    "luxon": "^3.4.4",
    "next": "^14.1.0",
    "pg": "^8.14.1",
    "safe-stable-stringify": "^2.5.0",
    "stripe": "^17.1.0",
    "ts-pattern": "^5.1.1",
    "twilio": "^5.4.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/flat": "^5.0.5",
    "@types/node": "^22.13.1",
    "@types/pg": "^8.11.11",
    "drizzle-kit": "^0.22.7",
    "typescript": "5.3.3",
    "vite": "^5.4.2",
    "vitest": "^2.0.5"
  }
}